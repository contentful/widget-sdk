.nav-bar
  height: 100%
  user-select: none

.nav-bar__list
  height: inherit
  .tooltip
    display: none !important

.nav-bar__list-item
  float: left
  display: inline-block
  height: inherit
  list-style-type: none
  margin: 0

.nav-bar__link
  $display-flexbox()
  align-items: center

.nav-bar__list-label
  margin-left: 4px

