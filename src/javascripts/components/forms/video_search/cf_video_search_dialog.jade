.modal-background
  .modal-dialog(cf-center-on=".modal-background")
    header.modal-dialog__header
      h1 Search for assets
      .close-button(ng-click="dialog.cancel()")
    .modal-dialog__content.video-dialog__content
      cf-video-search.video-dialog
    .modal-dialog__controls
      button.cfnext-btn-primary-action.modal-dialog__controls-confirm(ng-click="dialog.confirm(videoSearchController.getSelected())")
        | Insert selected
      a.modal-dialog__controls-cancel(ng-href="#" ng-click="dialog.cancel()" ng-show="dialog.cancelLabel")
        | {{dialog.cancelLabel}}

