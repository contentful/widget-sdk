  "scripts": {
    "start": "gulp all serve",
    "test": "TZ=UTC node run-tests.js",
    "test:once": "TZ=UTC node run-tests.js --once",
    "hint": "eslint src test tools tasks micro-backends",
    "jest": "TZ=UTC node run-tests-jest.js",
    "jest:coverage": "TZ=UTC node run-tests-jest.js --coverage",
    "jest:debug": "TZ=UTC node --inspect-brk run-tests-jest.js --runInBand",
    "jest:cli-debug": "TZ=UTC node debug run-tests-jest.js",
    "cloc": "cloc --read-lang-def .cloc.def src",
    "clean": "rm -rf node_modules",
    "codecov": "codecov",
    "bundle-analyzer": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./tools/webpack.analyzer.config.js",
    "prettier:debug": "prettier --config .prettierrc --no-editorconfig --debug-check \"./*.js\" \"{./src/javascripts,./tools,./test,./bin,./tasks,./config}/**/*.{js,json}\"",
    "prettier": "prettier --config .prettierrc --no-editorconfig --write \"./*.js\" \"{./src/javascripts,./tools,./test,./bin,./tasks,./config}/**/*.{js,json}\"",
    "postshrinkwrap": "if [ \"`uname`\" = \"Darwin\" ]; then sed -i '' 's/http:\\/\\//https:\\/\\//g' package-lock.json; else sed -i 's/http:\\/\\//https:\\/\\//g' package-lock.json; fi"
  },
