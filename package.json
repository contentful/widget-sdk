  "scripts": {
    "clean-logs-pacts": "rm -rf logs/ pacts/",
    "start": "node --max_old_space_size=6144 ./server.js",
    "start-debug": "node --max_old_space_size=6144 --inspect-brk ./server.js",
    "dev-staging": "NODE_ENV=dev UI_CONFIG=dev-on-staging node ./server.js",
    "dev-preview": "NODE_ENV=dev UI_CONFIG=dev-on-preview node ./server.js",
    "dev-production": "NODE_ENV=dev UI_CONFIG=dev-on-production node ./server.js",
    "cypress": "npm run clean-logs-pacts && npx cypress open",
    "test": "NODE_ENV=test NODE_OPTIONS=--max-old-space-size=6144 TZ=UTC node run-tests.js",
    "test:once": "NODE_ENV=test TZ=UTC node run-tests.js --once",
    "lint": "eslint src test tools micro-backends --ext '.ts,.tsx,.js,.jsx'",
    "jest": "TZ=UTC node run-tests-jest.js",
    "jest:coverage": "TZ=UTC node run-tests-jest.js --ci --coverage --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "jest:debug": "TZ=UTC node --inspect-brk run-tests-jest.js --runInBand",
    "jest:cli-debug": "TZ=UTC node debug run-tests-jest.js",
    "cloc": "cloc --read-lang-def .cloc.def src",
    "clean": "rm -rf node_modules",
    "codecov": "codecov",
    "cm": "git-cz",
    "bundle-analyzer": "node --max_old_space_size=6144 ./node_modules/webpack/bin/webpack.js --config ./tools/webpack.analyzer.config.js",
    "prettier:debug": "prettier --config .prettierrc --no-editorconfig --debug-check ./src/javascripts ./tools ./bin ./micro-backends ./test",
    "prettier": "prettier --config .prettierrc --no-editorconfig --write ./src/javascripts ./tools ./bin ./micro-backends ./test",
    "prettier:check": "prettier --config .prettierrc --no-editorconfig --check ./src/javascripts ./tools ./bin ./micro-backends ./test",
    "postshrinkwrap": "if [ \"`uname`\" = \"Darwin\" ]; then sed -i '' 's/http:\\/\\//https:\\/\\//g' package-lock.json; else sed -i 's/http:\\/\\//https:\\/\\//g' package-lock.json; fi",
    "cruise": "depcruise --output-type err-long --validate ./tools/dependency-cruise/dependency-cruise.js src/javascripts",
    "check-types": "tsc"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --ext '.ts,.tsx,.js,.jsx' --fix ",
      "git add"
    ],
    "*.{json,yml,html}": [
      "prettier --write",
      "git add"
    ]
  },
