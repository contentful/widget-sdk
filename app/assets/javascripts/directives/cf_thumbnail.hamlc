%img.thumbnail(ng-src="{{file.url}}?w={{thumbnailSize}}&h={{thumbnailSize}}" ng-show="isImage() && hasPreview()")
%i.ss-picture(ng-show="isImage() && !hasPreview()")
%i.ss-video(ng-show="isVideo()")
%i.ss-music(ng-show="isAudio()")
%i.ss-write(ng-show="isDocument()")
%i.ss-write(ng-show="isPdfdocument()")
%i.ss-box(ng-show="isArchive()")
%i.ss-write(ng-show="isPlaintext()")
%i.ss-desktop(ng-show="isCode()")
%i.ss-attach(ng-show="isAttachment()")
