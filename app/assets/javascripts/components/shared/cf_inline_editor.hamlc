%span.inline-editor-label(ng-if="!inlineEditing" ng-click="startEditing()") {{inner.text}}
%form.inline-editor-form(ng-if="inlineEditing" name="inlineForm" ng-submit="submitInlineForm()")
  %input.form-look(type="text" ng-model="inner.text" ng-blur="cancelInlineForm()" ng-keydown="keyDown($event)")
